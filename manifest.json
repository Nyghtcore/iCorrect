{
    "manifest_version": 2,
    "name": "iCorrect",
    "version": "1.0",
    "description": "Auto capitalizes the letter 'i' when it stands alone or in specific contractions.",
    "permissions": ["activeTab", "storage", "<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}
